<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginScreenController">
   <children>
      <Button fx:id="loginButton" layoutX="350.0" layoutY="350.0" mnemonicParsing="false" onAction="#onClick" prefHeight="30.0" prefWidth="100.0" text="Log In" />
      <Label fx:id="userNameLabel" alignment="CENTER_RIGHT" layoutX="150.0" layoutY="200.0" prefHeight="30.0" prefWidth="200.0" text="username:" textAlignment="RIGHT">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="passwordLabel" alignment="CENTER_RIGHT" layoutX="150.0" layoutY="250.0" prefHeight="30.0" prefWidth="200.0" text="password:" textAlignment="RIGHT">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <PasswordField fx:id="passwordField" layoutX="370.0" layoutY="250.0" prefHeight="30.0" prefWidth="150.0" />
      <TextField fx:id="userNameField" layoutX="370.0" layoutY="200.0" prefHeight="30.0" prefWidth="150.0" />
      <Label fx:id="warningLabel" alignment="CENTER" layoutX="250.0" layoutY="300.0" prefHeight="30.0" prefWidth="300.0" text="Label" textFill="RED">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="zoneLabel" alignment="CENTER" layoutX="275.0" layoutY="500.0" prefHeight="30.0" prefWidth="250.0" text="Label">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label fx:id="titleLabel" layoutX="50.0" layoutY="50.0" text="Login Form">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
